\begin_inset Formula 
\begin{align*}
f\left(x\right) & =\left(\frac{d-c}{b-a}\right)\left[\left(y-c\right)\left(\frac{b-a}{d-c}\right)+a-a\right]+c=\\
 & =\left(\frac{d-c}{b-a}\right)\left(y-c\right)\left(\frac{b-a}{d-c}\right)+c=y-c+c=y
\end{align*}

\end_inset

Ademas 
\begin_inset Formula $x\in C$
\end_inset

 pues: 
\begin_inset Formula 
\[
\begin{array}{ccccc}
c & \leq & y & \leq & d\\
0 & \leq & y-c & \leq & d-c\\
0 & \leq & \left(y-c\right)\left(\frac{b-a}{d-c}\right) & \leq & \left(d-c\right)\left(\frac{b-a}{d-c}\right)=b-a\\
a & \leq & x & \leq & b
\end{array}
\]

